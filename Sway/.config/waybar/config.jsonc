    "pulseaudio#input": {
       "format": "{format_source}",
       "format-source": "{volume}% <span color='#a8ff60'>ï„°</span>",
       "format-source-muted": "OFF <span color='#ff5233'>ï„±</span>",
       "format-icons": {
           "default": [
              "<span color='#a8ff60'>ï„°</span>",
              "<span color='#ffe28c'>ï„°</span>",
              "<span color='#ff5233'>ï„°</span>"
           ]
       },
       "scroll-step": 2,
       "smooth-scrolling-threshold": 1,
       "max-volume": 100,
       "on-click": "pavucontrol",
       "on-click-middle": "pactl set-source-mute @DEFAULT_SOURCE@ toggle",
       "on-scroll-up": "pactl set-source-volume @DEFAULT_SOURCE@ +1%",
       "on-scroll-down": "pactl set-source-volume @DEFAULT_SOURCE@ -1%",
    },
    "pulseaudio#output": {
