    "custom/power": {
        "format": "ï€‘",
        "tooltip": false,
        "on-click": "[ -z $(pgrep swaynag) ] && swaynag -t warning -m 'Power Menu Options' -B 'Logout' 'swaymsg exit' -B 'Restart' 'shutdown -r now' -b 'Shutdown' 'shutdown -h now'"
    },
    "custom/notification": {
       "tooltip": false,
       "format": "{icon}",
       "format-icons": {
         "notification": "ï‚¢<span foreground='red'><sup>ï‘„</sup></span>",
         "none": "ï‚¢",
         "dnd-notification": "ï‡·<span foreground='red'><sup>ï‘„</sup></span>",
